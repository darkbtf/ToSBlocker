{
  "manifest_version": 2,

  "name": "ToSBlocker",
  "description": "Block the annoying timeline virus.",
  "version": "0.0.0.1",

  "permissions": [
    "https://www.facebook.com/"
  ],

  "content_scripts": [{
      "js": ["jquery.min.js", "blocker.js"],
      "matches": ["*://www.facebook.com/*"],
      "run_at": "document_end"
  }
  ]

}